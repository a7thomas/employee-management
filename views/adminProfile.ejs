  <link rel="stylesheet" type="text/css" href="/stylesheets/styleProfile.css" />
  <input type="hidden" id="refreshed" value="no">
  <script type="text/javascript">
    onload = function() {
      var e = document.getElementById("refreshed");
      if (e.value == "no") e.value = "yes";
      else {
        e.value = "no";
        location.reload();
      }
    }
  </script>
</head>
